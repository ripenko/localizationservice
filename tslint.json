{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": [true, "ignore-same-line"],
        "object-literal-shorthand": [true, "never"],
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "max-line-length": false,
        "no-console": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}